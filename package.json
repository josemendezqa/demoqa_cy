{
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:update-snapshots": "cypress run --env updateSnapshot=true",
    "cy:stg-open": "cypress open --env envSelected=staging",
    "cy:dev-open": "cypress open --env envSelected=dev",
    "cy:local-open": "cypress open --env envSelected=local",
    "cy:stg-report": "cypress run --spec cypress/e2e/tests/**/specs/**.cy.js --env envSelected=staging --browser chrome --headless",
    "cy:run:stg:firefox": "cypress run --env configFile=staging --browser firefox",
    "cy:run:stg:chrome": "cypress run --env configFile=staging --browser chrome",
    "cy:run:stg:edge": "cypress run --env configFile=staging --browser edge"
  },
  "devDependencies": {
    "cypress": "^13.13.2",
    "cypress-mochawesome-reporter": "^3.5.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  }
}
